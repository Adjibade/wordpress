<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
  </startup>
  <appSettings>
    <add key="TokenEndPoint" value="https://login.microsoftonline.com/{0}/oauth2/v2.0/token"/>
    <add key="TenantId" value="[TenantId]"/>
    <add key="Scope" value="https://graph.microsoft.com/.default"/>
    <add key="ClientId" value="[ClientId]"/>
    <add key="ClientSecret" value="[ClientSecret]"/>
    <add key="GrantType" value="client_credentials"/>
    <add key="GraphEndPoint" value="https://graph.microsoft.com/v1.0/groups"/>
  </appSettings>
</configuration>